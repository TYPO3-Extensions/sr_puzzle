<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:lang/locallang_core.xlf:labels.generalTab</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.image>
						<TCEforms>
							<config>
								<type>inline</type>
								<foreign_table>sys_file_reference</foreign_table>
								<foreign_field>uid_foreign</foreign_field>
								<foreign_sortby>sorting_foreign</foreign_sortby>
								<foreign_table_field>tablenames</foreign_table_field>
								<foreign_match_fields type="array">
									<fieldname>image</fieldname>
								</foreign_match_fields>
								<foreign_label>uid_local</foreign_label>
								<foreign_selector>uid_local</foreign_selector>
								<maxItems>1</maxItems>
								<foreign_selector_fieldTcaOverride type="array">
									<config>
										<appearance>
											<elementBrowserType>file</elementBrowserType>
											<elementBrowserAllowed>gif,jpg,png</elementBrowserAllowed>
										</appearance>
									</config>
								</foreign_selector_fieldTcaOverride>
								<foreign_types type="array">
									<numIndex index="2" type="array">
										<showitem>--palette--;LLL:EXT:lang/locallang_tca.xlf:sys_file_reference.imageoverlayPalette;imageoverlayPalette,--palette--;;filePalette</showitem>
									</numIndex>
                                    				</foreign_types>
								<appearance type="array">
									<createNewRelationLinkTitle>LLL:EXT:cms/locallang_ttc.xlf:images.addFileReference</createNewRelationLinkTitle>
									<useSortable>0</useSortable>
									<showPossibleLocalizationRecords>1</showPossibleLocalizationRecords>
									<showRemovedLocalizationRecords>0</showRemovedLocalizationRecords>
									<showSynchronizationLink>0</showSynchronizationLink>
									<enabledControls type="array">
										<info>0</info>
										<new>0</new>
										<dragdrop>0</dragdrop>
										<hide>0</hide>
										<sort>0</sort>
										<delete>true</delete>
										<localize>0</localize>
										<levelLinksPosition>none</levelLinksPosition>
								 	</enabledControls>
									<headerThumbnail>
										<field>uid_local</field>
										<height>64</height>
										<width>64</width>
									</headerThumbnail>
								</appearance>
								<behaviour>
									<localizationMode>select</localizationMode>
									<localizeChildrenAtParentLocalization>1</localizeChildrenAtParentLocalization>
								</behaviour>
							</config>
					    	</TCEforms>
					</settings.image>
					<settings.puzzleWidth>
						<TCEforms>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:settings.puzzleWidth</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>5</size>
								<max>20</max>
								<range type="array">
									<upper>1000</upper>
									<lower>0</lower>
								</range>
							</config>
						</TCEforms>
					</settings.puzzleWidth>
					<settings.puzzleHeight>
						<TCEforms>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:settings.puzzleHeight</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>5</size>
								<max>20</max>
								<range type="array">
									<upper>1000</upper>
									<lower>0</lower>
								</range>
							</config>
						</TCEforms>
					</settings.puzzleHeight>
					<settings.pieceSize>
						<TCEforms>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:settings.pieceSize</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>5</size>
								<max>20</max>
								<range type="array">
									<upper>1000</upper>
									<lower>0</lower>
								</range>
							</config>
						</TCEforms>
					</settings.pieceSize>
					<settings.puzzleX>
						<TCEforms>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:settings.puzzleX</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>5</size>
								<max>20</max>
								<default>0</default>
								<range type="array">
									<upper>1000</upper>
									<lower>0</lower>
								</range>
							</config>
						</TCEforms>
					</settings.puzzleX>
					<settings.puzzleY>
						<TCEforms>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:settings.puzzleY</label>
							<config>
								<type>input</type>
								<eval>int</eval>
								<size>5</size>
								<max>20</max>
								<default>0</default>
								<range type="array">
									<upper>1000</upper>
									<lower>0</lower>
								</range>
							</config>
						</TCEforms>
					</settings.puzzleY>
					<settings.linkAfter>
						<TCEforms>
							<exclude>0</exclude>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:settings.linkAfter</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>3</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</settings.linkAfter>
					<settings.congratulationsLink>
						<TCEforms>
							<exclude>0</exclude>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:settings.congratulationsLink</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>3</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>
					</settings.congratulationsLink>
				</el>
			</ROOT>
		</sDEF>
		<sTemplate>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:view.template</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<view.templateRootPath>
						<TCEforms>
							<exclude>0</exclude>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:view.templateRootPath</label>
							<config>
								<type>input</type>
								<size>150</size>
								<default>EXT:sr_puzzle/Resources/Private/Templates/</default>
							</config>
						</TCEforms>
					</view.templateRootPath>
					<view.partialRootPath>
						<TCEforms>
							<exclude>0</exclude>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:view.partialRootPath</label>
							<config>
								<type>input</type>
								<size>150</size>
								<default>EXT:sr_puzzle/Resources/Private/Partials/</default>
							</config>
						</TCEforms>
					</view.partialRootPath>
					<view.layoutRootPath>
						<TCEforms>
							<exclude>0</exclude>
							<label>LLL:EXT:sr_puzzle/Resources/Private/Language/locallang.xlf:view.layoutRootPath</label>
							<config>
								<type>input</type>
								<size>150</size>
								<default>EXT:sr_puzzle/Resources/Private/Layouts/</default>
							</config>
						</TCEforms>
					</view.layoutRootPath>
				</el>
			</ROOT>
		</sTemplate>
	</sheets>
</T3DataStructure>